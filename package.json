{
  "name": "@codeyourfuture/cra-template",
  "version": "0.0.3",
  "description": "Create React App template for Code Your Future",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "template.json",
  "scripts": {
    "lint": "eslint template/",
    "test-app": "npm --prefix ./test-app run",
    "test-app:lint": "npm run test-app -- lint",
    "test-app:start": "concurrently -k -s first -n \"app,up?\" \"npm run test-app -- start\" \"wait-on -t 10000 http-get://localhost:3000\"",
    "pretest": "rimraf ./test-app && npx create-react-app@latest test-app --use-npm --template file:.",
    "test": "npm run test-app:lint && npm run test-app:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeYourFuture/cra-template.git"
  },
  "keywords": [
    "react",
    "template",
    "create-react-app"
  ],
  "author": "Jonathan Sharpe <mail@jonrshar.pe>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodeYourFuture/cra-template/issues"
  },
  "homepage": "https://github.com/CodeYourFuture/cra-template#readme",
  "files": [
    "template",
    "template.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "@codeyourfuture/standard"
    ],
    "settings": {
      "react": {
        "version": "latest"
      }
    }
  },
  "devDependencies": {
    "@codeyourfuture/eslint-config-standard": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "rimraf": "^3.0.2",
    "wait-on": "^5.1.0"
  }
}
